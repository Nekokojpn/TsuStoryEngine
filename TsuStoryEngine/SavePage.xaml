<Page x:Class="TsuStoryEngine.SavePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TsuStoryEngine"
      mc:Ignorable="d" 
      d:DesignHeight="720" d:DesignWidth="1269"
      Title="SavePage">

    <Grid x:Name="SGrid" Loaded="SGrid_Loaded">
        <Button x:Name="button" Content="SAVE" HorizontalAlignment="Left" Margin="0,665,0,0" VerticalAlignment="Top" Width="233" Height="55" FontSize="36" Click="button_Click"/>
        <TextBox x:Name="REMAIN" HorizontalAlignment="Left" Height="665" TextWrapping="Wrap" VerticalAlignment="Top" Width="1280" Background="#7F000000" Foreground="White"/>
        <ComboBox x:Name="comboBox1" HorizontalAlignment="Left" Margin="233,665,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Page>
